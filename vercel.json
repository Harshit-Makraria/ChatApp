{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/old-route",
        "destination": "/new-route",
        "permanent": true
      },
      {
        "source": "/another-old-route",
        "destination": "/another-new-route",
        "statusCode": 301
      }
    ]
  }
  